<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('./partials/head'); %>
    </head>
    <body data-barba="wrapper" class="mdui-theme-primary-indigo mdui-theme-accent-teal mdui-appbar-with-toolbar mdui-bottom-nav-fixed">
        <%- include('./partials/header'); %>
        
        <main data-barba="container" data-barba-namespace="<%= name %>" class="mdui-container hss-container">
            <!-- put here the content you wish to change
            between your pages, like your main content <h1> or <p> -->
            <%- include('./pages/' + name); %>
        </main>
        
        <%- include('./partials/footer', {
            name
        }); %>
        
        <%- include('./js/layout.ejs'); %>
        <script src="/js/js.js"></script>
    
        <!-- put here content that will not change
        between your pages, like <footer> -->
    </body>
</html>